<?xml version="1.0" encoding="UTF-8"?><layout title="Справочник контрагенты для выбора" view="view">
  <AnchorsPane background="#ffffff" cursor="0" name="view" prefHeight="300px" prefWidth="400px"/>
  <Button cursor="0" icon="icons/22x22/actions/dialog-apply.png" name="btnSelect" parent="view" prefHeight="30px" prefWidth="125px" text="Выбрать">
    <AnchorsPaneConstraints bottom="10px" height="30px" left="160px" width="125px"/>
  </Button>
  <Button cursor="0" focusable="false" horizontalTextPosition="0" icon="icons/22x22/actions/new-view-refresh.png" name="btnReq" parent="view" prefHeight="30px" prefWidth="35px" toolTipText="Обновить" verticalTextPosition="3">
    <AnchorsPaneConstraints bottom="10px" height="30px" left="10px" width="35px"/>
  </Button>
  <ModelGrid cursor="0" data="qAllFirms" name="modelGrid" parent="view" prefHeight="250px" prefWidth="400px">
    <AnchorsPaneConstraints bottom="50px" left="0px" right="0px" top="0px"/>
    <ServiceGridColumn name="colService" resizable="true"/>
    <ModelGridColumn field="company_name" name="colCompany_name" title="Наименование" width="180px">
      <ModelFormattedField format=""/>
    </ModelGridColumn>
    <ModelGridColumn name="colInn" title="Инн" width="179px">
      <ModelSpin max="1.7976931348623157E308" min="-1.7976931348623157E308"/>
    </ModelGridColumn>
  </ModelGrid>
  <Button cursor="0" name="btnCancel" parent="view" prefHeight="30px" prefWidth="100px" text="Отмена">
    <AnchorsPaneConstraints bottom="10px" height="30px" right="10px" width="100px"/>
  </Button>
</layout>
