    <ServiceGridColumn name="colService" resizable="true"/>
    <ModelGridColumn field="type_name" name="colType_name" title="Наименование" width="359px">
      <ModelFormattedField format=""/>
    </ModelGridColumn>
  </ModelGrid>
  <Button cursor="0" name="btnSelect" parent="view" prefHeight="30px" prefWidth="100px" text="Выбрать">
    <AnchorsPaneConstraints bottom="7px" height="30px" left="180px" width="100px"/>
  </Button>
  <Button cursor="0" name="btnCancel" parent="view" prefHeight="30px" prefWidth="100px" text="Отмена">
    <AnchorsPaneConstraints bottom="7px" height="30px" right="10px" width="100px"/>
  </Button>
</layout>
