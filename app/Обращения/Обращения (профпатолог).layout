<?xml version="1.0" encoding="UTF-8"?><layout title="Обращения" view="view">
  <AnchorsPane background="#eaeaea" cursor="0" name="view" prefHeight="486px" prefWidth="940px"/>
  <AnchorsPane background="#eaeaea" cursor="0" name="panel" parent="view" prefHeight="486px" prefWidth="230px">
    <AnchorsPaneConstraints bottom="0px" left="0px" top="0px" width="230px"/>
  </AnchorsPane>
  <ModelCombo background="#ffffff" cursor="0" displayField="company_name" displayList="qAllFirms" name="mcWorkPlace" parent="panel" prefHeight="20px" prefWidth="210px">
    <AnchorsPaneConstraints height="20px" left="10px" top="60px" width="210px"/>
  </ModelCombo>
  <ModelFormattedField cursor="0" format="" name="tfFirstName" parent="panel" prefHeight="20px" prefWidth="210px">
    <AnchorsPaneConstraints height="20px" left="10px" top="250px" width="210px"/>
  </ModelFormattedField>
  <ModelFormattedField cursor="0" format="" name="tfSurname" parent="panel" prefHeight="20px" prefWidth="210px">
    <AnchorsPaneConstraints height="20px" left="10px" top="290px" width="210px"/>
  </ModelFormattedField>
  <Label cursor="0" name="label" parent="panel" prefHeight="20px" prefWidth="70px" text="Компания">
    <font family="Monospaced" size="11" style="1"/>
    <AnchorsPaneConstraints height="20px" left="10px" top="40px" width="70px"/>
  </Label>
  <Label cursor="0" name="label2" parent="panel" prefHeight="20px" prefWidth="70px" text="Имя">
    <AnchorsPaneConstraints height="20px" left="10px" top="230px" width="70px"/>
  </Label>
  <Label cursor="0" name="label3" parent="panel" prefHeight="20px" prefWidth="70px" text="Фамилия">
    <AnchorsPaneConstraints height="20px" left="10px" top="270px" width="70px"/>
  </Label>
  <Button cursor="0" name="btnApplyFilter" parent="panel" prefHeight="30px" prefWidth="110px" text="Применить">
    <AnchorsPaneConstraints height="30px" left="10px" top="340px" width="110px"/>
  </Button>
  <Label cursor="0" name="label4" parent="panel" prefHeight="40px" prefWidth="210px" text="Фильтр:">
    <font family="Monospaced" size="18" style="0"/>
    <AnchorsPaneConstraints height="40px" left="10px" top="0px" width="210px"/>
  </Label>
  <Label cursor="0" name="label6" parent="panel" prefHeight="20px" prefWidth="70px" text="Назначения">
    <font family="Monospaced" size="11" style="1"/>
    <AnchorsPaneConstraints height="20px" left="10px" top="90px" width="70px"/>
  </Label>
  <Label cursor="0" name="label7" parent="panel" prefHeight="20px" prefWidth="50px" text="Статус">
    <AnchorsPaneConstraints height="20px" left="10px" top="120px" width="50px"/>
  </Label>
  <ModelCombo background="#ffffff" cursor="0" displayField="status_name" displayList="qObrStatus" name="mcTreatStatus" parent="panel" prefHeight="20px" prefWidth="160px">
    <AnchorsPaneConstraints height="20px" left="60px" top="120px" width="160px"/>
  </ModelCombo>
  <Label cursor="0" name="label8" parent="panel" prefHeight="20px" prefWidth="50px" text="С">
    <AnchorsPaneConstraints height="20px" left="10px" top="150px" width="50px"/>
  </Label>
  <Label cursor="0" name="label9" parent="panel" prefHeight="20px" prefWidth="50px" text="По">
    <AnchorsPaneConstraints height="20px" left="10px" top="180px" width="50px"/>
  </Label>
  <ModelDate background="#ffffff" cursor="0" dateFormat="dd/MM/yyyy" name="mdTreatStart" parent="panel" prefHeight="20px" prefWidth="160px">
    <AnchorsPaneConstraints height="20px" left="60px" top="150px" width="160px"/>
  </ModelDate>
  <ModelDate background="#ffffff" cursor="0" dateFormat="dd/MM/yyyy" name="mdTreatEnd" parent="panel" prefHeight="20px" prefWidth="160px">
    <AnchorsPaneConstraints height="20px" left="60px" top="180px" width="160px"/>
  </ModelDate>
  <Label cursor="0" name="label10" parent="panel" prefHeight="20px" prefWidth="210px" text="Фильтр по ФИО">
    <font family="Monospaced" size="11" style="1"/>
    <AnchorsPaneConstraints height="20px" left="10px" top="210px" width="210px"/>
  </Label>
  <Button cursor="0" name="btnClearFilter" parent="panel" prefHeight="30px" prefWidth="90px" text="Сбросить">
    <AnchorsPaneConstraints height="30px" left="130px" top="340px" width="90px"/>
  </Button>
  <ToolBar background="#eaeaea" cursor="0" name="toolBar" parent="view" prefHeight="80px" prefWidth="620px">
    <AnchorsPaneConstraints height="80px" left="230px" right="90px" top="0px"/>
  </ToolBar>
  <Button cursor="0" focusable="false" horizontalTextPosition="0" icon="icons/48x48/actions/document-edit.png" name="btnEditPatient" parent="toolBar" prefHeight="80px" prefWidth="90px" text="Карточка" verticalTextPosition="3">
    <font family="Monospaced" size="11" style="0"/>
  </Button>
  <Button cursor="0" focusable="false" horizontalTextPosition="0" icon="icons/48x48/actions/application-exit.png" name="btnExit" parent="view" prefHeight="80px" prefWidth="90px" text="Выход" verticalTextPosition="3">
    <font family="Monospaced" size="11" style="0"/>
    <AnchorsPaneConstraints height="80px" right="0px" top="0px" width="90px"/>
  </Button>
  <SplitPane cursor="0" dividerLocation="250" foreground="#000000" leftComponent="panel2" name="splitPane" orientation="0" parent="view" prefHeight="406px" prefWidth="710px" rightComponent="pnlObr">
    <font family="Monospaced" size="11" style="0"/>
    <AnchorsPaneConstraints bottom="0px" left="230px" right="0px" top="80px"/>
  </SplitPane>
  <AnchorsPane background="#ffffff" cursor="0" name="panel2" parent="splitPane" prefHeight="248px" prefWidth="706px"/>
  <AnchorsPane background="#eaeaea" cursor="0" name="panel1" parent="panel2" prefHeight="20px" prefWidth="706px">
    <AnchorsPaneConstraints bottom="0px" height="20px" left="0px" right="0px"/>
  </AnchorsPane>
  <Label cursor="0" name="label1" parent="panel1" prefHeight="20px" prefWidth="110px" text="Всего записей:">
    <AnchorsPaneConstraints height="20px" left="10px" top="0px" width="110px"/>
  </Label>
  <Label cursor="0" name="lbPatientsCount" parent="panel1" prefHeight="20px" prefWidth="70px">
    <AnchorsPaneConstraints height="20px" left="120px" top="0px" width="70px"/>
  </Label>
  <Label cursor="0" name="label5" parent="panel1" prefHeight="20px" prefWidth="140px" text="Записей выбрано:">
    <AnchorsPaneConstraints height="20px" left="200px" top="0px" width="140px"/>
  </Label>
  <Label cursor="0" name="lbSelectedCount" parent="panel1" prefHeight="20px" prefWidth="70px">
    <AnchorsPaneConstraints height="20px" left="340px" top="0px" width="70px"/>
  </Label>
  <Button cursor="0" name="btnSelAll" parent="panel1" prefHeight="20px" prefWidth="110px" text="Выбрать все">
    <AnchorsPaneConstraints height="20px" left="410px" top="0px" width="110px"/>
  </Button>
  <ModelGrid cursor="0" data="qAllPatientTreatmentsStatuses" name="modelGrid" parent="panel2" prefHeight="228px" prefWidth="706px">
    <AnchorsPaneConstraints bottom="20px" left="0px" right="0px" top="0px"/>
    <ServiceGridColumn name="colService" resizable="true"/>
    <ModelGridColumn field="usl_usluga" name="colUsluga_id" title="Обращение" width="75px">
      <ModelCombo displayField="usl_name" displayList="qUslugaById"/>
    </ModelGridColumn>
    <ModelGridColumn field="treat_date" name="colTreat_date" title="Дата" width="75px">
      <ModelDate dateFormat="dd/MM/yyyy"/>
    </ModelGridColumn>
    <ModelGridColumn field="t_status" name="colTreat_status" title="Статус" width="75px">
      <ModelCombo displayField="status_name" displayList="qObrStatus"/>
    </ModelGridColumn>
    <ModelGridColumn field="surname" name="colSurname" title="Фамилия" width="75px">
      <ModelFormattedField format=""/>
    </ModelGridColumn>
    <ModelGridColumn field="firstname" name="colFirstname" title="Имя" width="75px">
      <ModelFormattedField format=""/>
    </ModelGridColumn>
    <ModelGridColumn field="patronymic" name="colPatronymic" title="Отчество" width="75px">
      <ModelFormattedField format=""/>
    </ModelGridColumn>
    <ModelGridColumn field="sex" name="colSex" title="Пол" width="75px">
      <ModelFormattedField format=""/>
    </ModelGridColumn>
    <ModelGridColumn field="date_oft_birth" name="colDate_oft_birth" title="Дата рожд." width="75px">
      <ModelDate dateFormat="dd/MM/yyyy"/>
    </ModelGridColumn>
    <ModelGridColumn field="p_company" name="colCompany_id" title="Компания" width="75px">
      <ModelCombo displayField="company_name" displayList="qAllFirms"/>
    </ModelGridColumn>
    <ModelGridColumn field="p_job" name="colJob_id" title="Должность" width="75px">
      <ModelCombo displayField="job_title" displayList="qManJob"/>
    </ModelGridColumn>
  </ModelGrid>
  <BorderPane background="#ffffff" cursor="0" name="pnlObr" parent="splitPane" prefHeight="145px" prefWidth="706px"/>
</layout>
