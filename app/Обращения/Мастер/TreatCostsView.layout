<?xml version="1.0" encoding="UTF-8"?><layout title="" view="view">
  <AnchorsPane background="#ffffff" cursor="0" name="view" prefHeight="300px" prefWidth="400px"/>
  <ModelGrid cursor="0" deletable="false" name="mgCosts" parent="view" prefHeight="260px" prefWidth="400px">
    <AnchorsPaneConstraints bottom="0px" left="0px" right="0px" top="40px"/>
    <ServiceGridColumn name="colService" resizable="true"/>
    <ModelGridColumn field="usl_id" name="colUsl_name" title="Название " width="75px">
      <ModelCombo displayField="usl_name" displayList="qUslugaById" list="false"/>
    </ModelGridColumn>
    <ModelGridColumn field="usl_sex" name="colSex" title="Пол" width="75px">
      <ModelCombo displayField="man_sex_id" displayList="qSex"/>
    </ModelGridColumn>
    <ModelGridColumn field="usl_per_type" name="colPer_type" title="Переодичность" width="75px">
      <ModelCombo displayField="type_name" displayList="qUslPeriodicType"/>
    </ModelGridColumn>
    <ModelGridColumn field="lim_age_type" name="colVozrastType" title="Тип Возраста" width="75px">
      <ModelCombo displayField="description" displayList="qUsiLimAgeType"/>
    </ModelGridColumn>
    <ModelGridColumn field="limitation_age" name="colVozrast" title="Возраст" width="75px">
      <ModelFormattedField format=""/>
    </ModelGridColumn>
    <ModelGridColumn field="cost" name="colCost" title="Цена" width="75px">
      <ModelTextArea/>
    </ModelGridColumn>
  </ModelGrid>
  <ToolBar cursor="0" name="toolBar1" parent="view" prefHeight="40px" prefWidth="400px">
    <AnchorsPaneConstraints height="40px" left="0px" right="0px" top="0px"/>
  </ToolBar>
  <Button cursor="0" focusable="false" horizontalTextPosition="0" name="btnDoublePrice" parent="toolBar1" prefHeight="40px" prefWidth="100px" text="Дублировать" verticalTextPosition="3"/>
  <Button cursor="0" focusable="false" horizontalTextPosition="0" name="btnSaveToPrice" parent="toolBar1" prefHeight="40px" prefWidth="100px" text="Сохранить в прайс-лист" verticalTextPosition="3"/>
  <Button cursor="0" focusable="false" horizontalTextPosition="0" name="btnSaveToContract" parent="toolBar1" prefHeight="40px" prefWidth="100px" text="Сохранить в договор" verticalTextPosition="3"/>
</layout>
