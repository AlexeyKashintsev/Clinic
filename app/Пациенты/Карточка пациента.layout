<?xml version="1.0" encoding="UTF-8"?><layout title="Карта пациента" view="view">
  <AnchorsPane background="#ffffff" cursor="0" name="view" prefHeight="451px" prefWidth="400px"/>
  <Label cursor="0" name="label" parent="view" prefHeight="20px" prefWidth="50px" text="Фамилия">
    <AnchorsPaneConstraints height="20px" left="10px" top="10px" width="50px"/>
  </Label>
  <Label cursor="0" name="label1" parent="view" prefHeight="20px" prefWidth="50px" text="Имя">
    <AnchorsPaneConstraints height="20px" left="10px" top="40px" width="50px"/>
  </Label>
  <Label cursor="0" name="label2" parent="view" prefHeight="20px" prefWidth="50px" text="Отчество">
    <AnchorsPaneConstraints height="20px" left="10px" top="70px" width="50px"/>
  </Label>
  <Label cursor="0" name="label3" parent="view" prefHeight="20px" prefWidth="50px" text="Пол">
    <AnchorsPaneConstraints height="20px" right="120px" top="10px" width="50px"/>
  </Label>
  <Label cursor="0" name="label4" parent="view" prefHeight="20px" prefWidth="90px" text="Дата рождения">
    <AnchorsPaneConstraints height="20px" right="80px" top="40px" width="90px"/>
  </Label>
  <ModelFormattedField cursor="0" data="qPatientById" field="cursor.surname" format="" name="tfSurname" parent="view" prefHeight="20px" prefWidth="150px">
    <AnchorsPaneConstraints height="20px" left="70px" right="180px" top="10px"/>
  </ModelFormattedField>
  <ModelFormattedField cursor="0" data="qPatientById" field="cursor.firstname" format="" name="tfName" parent="view" prefHeight="20px" prefWidth="150px">
    <AnchorsPaneConstraints height="20px" left="70px" right="180px" top="40px"/>
  </ModelFormattedField>
  <ModelFormattedField cursor="0" data="qPatientById" field="cursor.patronymic" format="" name="tfPatronymic" parent="view" prefHeight="20px" prefWidth="150px">
    <AnchorsPaneConstraints height="20px" left="70px" right="180px" top="70px"/>
  </ModelFormattedField>
  <ModelCombo cursor="0" data="qPatientById" displayField="sex_short" displayList="qSex" field="cursor.sex_list" name="ddSex" parent="view" prefHeight="20px" prefWidth="120px">
    <AnchorsPaneConstraints height="20px" right="10px" top="10px" width="120px"/>
  </ModelCombo>
  <ModelDate cursor="0" data="qPatientById" dateFormat="dd MMM yyyy" field="cursor.date_oft_birth" name="mdDateOfBirth" parent="view" prefHeight="20px" prefWidth="160px">
    <AnchorsPaneConstraints height="20px" right="10px" top="70px" width="160px"/>
  </ModelDate>
  <Button cursor="0" name="btnSave" parent="view" prefHeight="30px" prefWidth="100px" text="Сохранить">
    <AnchorsPaneConstraints bottom="12px" height="30px" right="100px" width="100px"/>
  </Button>
  <Button cursor="0" name="btnCancel" parent="view" prefHeight="30px" prefWidth="80px" text="Отмена">
    <AnchorsPaneConstraints bottom="12px" height="30px" right="10px" width="80px"/>
  </Button>
  <TabbedPane cursor="0" name="tabbedPane" parent="view" prefHeight="299px" prefWidth="380px">
    <AnchorsPaneConstraints bottom="52px" left="10px" right="10px" top="100px"/>
  </TabbedPane>
  <AnchorsPane background="#ffffff" cursor="0" name="pnlTreats" parent="tabbedPane" prefHeight="271px" prefWidth="375px" visible="true">
    <TabbedPaneConstraints tabTitle="Обращения"/>
  </AnchorsPane>
  <ToolBar cursor="0" name="toolBar" parent="pnlTreats" prefHeight="33px" prefWidth="375px">
    <AnchorsPaneConstraints height="33px" left="0px" right="0px" top="0px"/>
  </ToolBar>
  <Button cursor="0" focusable="false" horizontalTextPosition="0" icon="icons/22x22/actions/appointment-new.png" name="btnNewTreat" parent="toolBar" prefHeight="33px" prefWidth="40px" verticalTextPosition="3"/>
  <ModelGrid cursor="0" data="qTreatByPatient" deletable="false" editable="false" insertable="false" name="mgTreats" parent="pnlTreats" prefHeight="237px" prefWidth="375px">
    <AnchorsPaneConstraints bottom="0px" left="0px" right="0px" top="34px"/>
    <ModelGridColumn field="treat_date" name="colTreatDate" title="Дата" width="120px">
      <ModelDate dateFormat="DD MMM YYYY"/>
    </ModelGridColumn>
    <ModelGridColumn field="usl_string" name="colUslugi" title="Услуга" width="120px">
      <ModelFormattedField/>
    </ModelGridColumn>
    <ModelGridColumn field="treat_status" name="colStatus" title="Состояние" width="119px">
      <ModelCombo displayField="status_name" displayList="qObrStatus"/>
    </ModelGridColumn>
  </ModelGrid>
  <AnchorsPane background="#ffffff" cursor="0" name="pnlWorkplace" parent="tabbedPane" prefHeight="271px" prefWidth="375px">
    <TabbedPaneConstraints tabTitle="Работа и вредность"/>
  </AnchorsPane>
  <ModelGrid cursor="0" data="qWorkPlaceByPatient" name="mgWorkPlace" parent="pnlWorkplace" prefHeight="100px" prefWidth="375px">
    <AnchorsPaneConstraints bottom="134px" left="0px" right="0px" top="37px"/>
    <ModelGridColumn field="company_id" name="colCompany" title="Компания" width="167px">
      <ModelCombo displayField="company_name" displayList="qAllFirms"/>
    </ModelGridColumn>
    <ModelGridColumn field="job_id" name="colJobTitle" title="Должность" width="167px">
      <ModelCombo displayField="job_title" displayList="qManJob"/>
    </ModelGridColumn>
    <ModelGridColumn field="active" maxWidth="25px" minWidth="25px" name="colActive" preferredWidth="25px" title="Текущее" width="25px">
      <ModelCheckBox/>
    </ModelGridColumn>
  </ModelGrid>
  <Label cursor="0" name="label5" parent="pnlWorkplace" prefHeight="20px" prefWidth="90px" text="Место работы">
    <AnchorsPaneConstraints height="20px" left="10px" top="6px" width="90px"/>
  </Label>
  <Button cursor="0" icon="icons/22x22/actions/insert-object.png" name="btnAddWorkPlace" parent="pnlWorkplace" prefHeight="30px" prefWidth="170px" text="Новое место работы">
    <AnchorsPaneConstraints height="30px" right="5px" top="1px" width="170px"/>
  </Button>
  <Label cursor="0" name="label6" parent="pnlWorkplace" prefHeight="20px" prefWidth="70px" text="Вредности">
    <AnchorsPaneConstraints bottom="103px" height="20px" left="10px" width="70px"/>
  </Label>
  <ModelGrid cursor="0" data="qHazardsByManJob" name="modelGrid" parent="pnlWorkplace" prefHeight="91px" prefWidth="375px">
    <AnchorsPaneConstraints bottom="0px" height="91px" left="0px" right="0px"/>
    <ModelGridColumn field="haz_short_name" name="column" width="359px">
      <ModelFormattedField/>
    </ModelGridColumn>
  </ModelGrid>
  <Button cursor="0" icon="icons/22x22/actions/insert-object.png" name="btnAddHazard" parent="pnlWorkplace" prefHeight="30px" prefWidth="30px">
    <AnchorsPaneConstraints bottom="98px" height="30px" right="35px" width="30px"/>
  </Button>
  <Button cursor="0" icon="icons/22x22/actions/list-remove.png" name="btnDelHazard" parent="pnlWorkplace" prefHeight="30px" prefWidth="30px">
    <AnchorsPaneConstraints bottom="98px" height="30px" right="5px" width="30px"/>
  </Button>
</layout>
